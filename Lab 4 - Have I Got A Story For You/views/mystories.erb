<section class="main">
  <h2><%= session[:username] %>'s Stories</h2>
  <table class="gridtable">
    <tr>
      <th>Story Name</th>
      <th>Date Created</th>
      <th>Date Modified</th>
    </tr>
    <% @stories.each do |i| %>
        <%= '<tr>' %>
        <%= '<td><a href="/view_story/' + i["storyId"].to_s + '">' + i["storyName"].to_s + '</a></td>' %>
        <%= '<td>' + i["dateCreated"].to_s + '</td>' %>
        <%= '<td>' + i["dateModified"].to_s + '</td>' %>
    <% end %>
  </table>
  <a href="/create_story">Create Story</a>
</section>